type: component.default
children:
  - type: component.card
    options:
      # color: color9
      children:
        - type: component.view
          options:
            style:
              gap: small
              flex: 
                direction: row
            children:
              - type: component.view
                options:
                  style: 
                    width: 160
                    gap: minimal
                  children:
                    - type: component.image
                      options:
                        resizeMode: contain
                        height: 160
                        source:
                          uri: =@ctx.inputs.image
                        
                    - type: component.view
                      options:
                        style:
                          padding: 
                            # left: medium
                            top: medium
                          
                        children:
                          
                          - type: component.text
                            options:
                              value: Sunny
                              # align: center
                              size: large
                              # emphasis: high
                              # weight: regular
                          - type: component.text
                            options:
                              value: Sunday, 12 Feb
                        
                          
              - type: component.view
                options:
                  style: 
                    
                    alignItems: center
                    flex: 
                      grow: 1
                    padding: 
                      top: large
                    # gap: small
                    
                  children:
                
                    
                    - type: component.text
                      options:
                        value: =@ctx.inputs.max & 'C'
                        size: extra-large
                        # emphasis: high
                      
                    - type: component.text
                      options:
                        value: Feels like 25Â°
                        size: regular
                        weight: regular
                        
                    - type: component.view
                      options:
                        style:
                          # background:
                          #   color: color1
                          justifyContent: center
                          alignItems: flex-end
                          padding: 
                            top: medium
                            left: medium
                          gap: small
                          flex: 
                            grow: 1
                            direction: row
                        children:
                          - type: component.view
                            options:
                              style:
                                
                                justifyContent: center
                                alignItems: center
                                flex: 
                                  grow: 1
                              children:
                                - type: component.text
                                  options:
                                    value: 6:00 am
                                    weight: semi-bold
                                    size: small
                                - type: component.view
                                  options:
                                    style:
                                      padding: 
                                        top: small
                                      # emphasis: high
                                      justifyContent: center
                                      alignItems: center
                                    children:
                                      - type: component.avatar
                                        options:
                                          size: small
                                          title: ""
                                          uri: data:image/png;base64,
                                          
                                      - type: component.text
                                        options:
                                          value: Sunrise
                                          size: small
                          - type: component.view
                            options:
                              style:
                                justifyContent: center
                                alignItems: center
                                flex: 
                                  grow: 1
                                
                              children:
                                - type: component.text
                                  options:
                                    value: 8:00 pm
                                    weight: semi-bold
                                    size: small
                                - type: component.view
                                  options:
                                    style:
                                      padding: 
                                        top: small
                                      # emphasis: high
                                      justifyContent: center
                                      alignItems: center
                                    children:
                                      
                                      - type: component.avatar
                                        options:
                                          size: small
                                          title: ""
                                          uri: data:image/png;base64,
                                      - type: component.text
                                        options:
                                          value: Sunset
                                          size: small
  - type: component.view
    options:
      style:
        emphasis: medium
        flex: 
          direction: row
        padding:  
          top: large
          left: small
          # bottom: medium
        alignContent: center
        gap: small
        
      children:
        - type: component.icon
          options:
            icon: calendar
            size: small
        - type: component.text
          options:
            value: 7-day forecast
            size: regular