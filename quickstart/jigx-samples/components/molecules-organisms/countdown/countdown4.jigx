type: component.default
children:
  - type: component.card
    options:
      color: color5
      children:
        - type: component.countdown
          options:
            expiresAt: =$fromMillis($toMillis($now()) + (14 * 24 * 60 * 60 * 1000))
            labels:
              position: bottom
            size: extra-large

