type: datasource.sqlite
options:
  provider: DATA_PROVIDER_DYNAMIC
  entities:
    - entity: default/location
  query: |
    SELECT
      '$.id',
      '$.address',
      json_extract(data, '$.adress-us') as addressAus,
      '$.category',  
      '$.lat',
      '$.lng'
    FROM [default/location] WHERE '$.category' = "location"